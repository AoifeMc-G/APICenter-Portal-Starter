# Environment variables for Front Door configuration
# Add these to your .env file or deployment environment

# Your Front Door endpoint (the azurefd.net URL)
REACT_APP_FRONT_DOOR_ENDPOINT=your-front-door-endpoint.azurefd.net

# Your Front Door ID (for validation)
REACT_APP_FRONT_DOOR_ID=your-front-door-id

# Enable Front Door validation
REACT_APP_ENFORCE_FRONT_DOOR=true

# Enable automatic redirect to Front Door if direct access is detected
REACT_APP_ENFORCE_FRONT_DOOR_REDIRECT=false
